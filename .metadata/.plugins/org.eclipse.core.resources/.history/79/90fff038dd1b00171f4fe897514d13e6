package oop.polymorphism;

public class TestShape {

	public static void main(String[] args) {
		ShapeCalculator shape = new ShapeCalculator();
		int resultOfLine = shape.areaOfLand(10, 15);
		System.out.println(resultOfLine);
		int resultOfTriangle = shape.areaOfLand(10, 15, 12);
		System.out.println(resultOfTriangle);
		int resultOfRectangle = shape.areaOfLand(10, 15, 12);
		System.out.println(resultOfRectangle);

	}

}
